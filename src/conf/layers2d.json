[{
    "name": "배경 지도",
    "group": "baseImagery",
    "children": [{
        "name": "브이월드",
        "type": "radio",
        "group": "baseImagery",
        "show": true,
        "layer": "tileLayer",
        "provider": "koreaProvider",
        "src": "VWorld.Street",
        "options": {}
    }, {
        "name": "Bing Maps Road",
        "type": "radio",
        "group": "baseImagery",
        "provider": "BingMapsImageryProvider",
        "options": {
            "url": "https://dev.virtualearth.net",
            "mapStyle": "Road"
        }
    }, {
        "name": "ArcGIS World Street Maps",
        "type": "radio",
        "group": "baseImagery",
        "provider": "ArcGisMapServerImageryProvider",
        "options": {
            "url": "https://services.arcgisonline.com/ArcGIS/rest/services/World_Street_Map/MapServer"
        }
    }, {
        "name": "OpenStreetMaps",
        "type": "radio",
        "group": "baseImagery",
        "provider": "OpenStreetMapImageryProvider",
        "options": {}
    }, {
        "name": "Stamen Maps",
        "type": "radio",
        "group": "baseImagery",
        "provider": "OpenStreetMapImageryProvider",
        "options": {
            "url": "https://stamen-tiles.a.ssl.fastly.net/watercolor/",
            "fileExtension": "jpg",
            "credit": "Map tiles by Stamen Design, under CC BY 3.0. Data by OpenStreetMap, under CC BY SA."
        }
    }, {
        "name": "Natural Earth II (local)",
        "type": "radio",
        "group": "baseImagery",
        "provider": "TileMapServiceImageryProvider",
        "options": {
            "url": "http://localhost:8081/Assets/Textures/NaturalEarthII"
        }
    }, {
        "name": "USGS Shaded Relief (via WMTS)",
        "type": "radio",
        "group": "baseImagery",
        "provider": "WebMapTileServiceImageryProvider",
        "options": {
            "url": "http://basemap.nationalmap.gov/arcgis/rest/services/USGSShadedReliefOnly/MapServer/WMTS",
            "layer": "USGSShadedReliefOnly",
            "style": "default",
            "format": "image/jpeg",
            "tileMatrixSetID": "default028mm",
            "maximumLevel": 19,
            "credit": "U. S. Geological Survey"
        }
    }, {
        "name": "Ion World",
        "type": "radio",
        "group": "baseImagery",
        "provider": "createWorldImagery",
        "options": {
            "style": 3
        }
    }]
}, {
    "name": "지형 지도",
    "group": "terrian",
    "children": [{
        "name": "World Terrian",
        "type": "radio",
        "group": "terrian",
        "provider": "createWorldTerrain",
        "show": true,
        "options": {
            "requestVertexNormals": true,
            "requestWaterMask": true
        }
    }, {
        "name": "South Korea Terrian",
        "type": "radio",
        "group": "terrian",
        "provider": "CesiumTerrainProvider",
        "options": {
            "proxy": "/GVS/proxy.jsp",
            "url": "http://192.168.1.101:38080/tilesets/srtm/",
            "requestVertexNormals": true,
            "requestWaterMask": true
        }
    }]
